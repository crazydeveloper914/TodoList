{
  "name": "coc-todolist",
  "version": "1.0.0",
  "description": "todolist/task manager extension for coc.nvim",
  "main": "lib/index.js",
  "publisher": "voldikss",
  "keywords": [
    "coc.nvim",
    "todo"
  ],
  "engines": {
    "coc": "^0.0.70"
  },
  "scripts": {
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "todolist.maxsize": {
          "type": "number",
          "default": 5000,
          "description": "maxsize of todolist"
        },
        "todolist.autoUpload": {
          "type": "boolean",
          "default": false,
          "description": "sync your todo every day when coc was started"
        },
        "todolist.monitor": {
          "type": "boolean",
          "default": false,
          "description": "whether to monitor the ddl and reminder you at the time"
        },
        "todolist.reminder.background": {
          "type": "string",
          "default": "",
          "description": "notification background"
        },
        "todolist.reminder.winblend": {
          "type": "number",
          "default": 100,
          "description": "opacity of reminder floating window"
        },
        "todolist.reminder.width": {
          "type": "number",
          "default": 30,
          "description": "width of reminder floating window"
        },
        "todolist.reminder.notify": {
          "type": "string",
          "default": "floating",
          "description": "width of reminder floating window",
          "enum": [
            "floating",
            "virtual",
            "echo",
            "none"
          ]
        }
      }
    },
    "commands": [
      {
        "title": "todolist",
        "command": "todolist.new"
      },
      {
        "title": "todolist",
        "command": "todolist.update"
      },
      {
        "title": "upload todolist to gist",
        "command": "todolist.upload"
      },
      {
        "title": "download todolist from gist",
        "command": "todo.download"
      },
      {
        "title": "export todolist to markdown or json file",
        "command": "todolist.export"
      }
    ]
  },
  "author": "dyzplus@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.18",
    "@octokit/rest": "^16.28.4",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.0.10",
    "@types/uuid": "^3.4.4",
    "coc.nvim": "^0.0.73",
    "js-yaml": "^3.13.1",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "uuid": "^3.3.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "log4js": "^4.5.1"
  }
}
